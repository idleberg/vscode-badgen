{
    "name": "badgen",
    "displayName": "Badgen",
    "description": "Snippets to quickly insert Badgen badges into Markdown documents",
    "version": "0.0.0",
    "publisher": "idleberg",
    "license": "MIT",
    "author": {
        "name": "Jan T. Sott",
        "url": "http://github.com/idleberg"
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "precommit": "npm run test",
        "test": "vscode-linter",
        "vscode:prepublish": "gulp"
    },
    "keywords": [
        "badge",
        "badges",
        "badgen",
        "markdown"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/idleberg/vscode-badgen"
    },
    "homepage": "https://github.com/idleberg/vscode-badgen#readme",
    "bugs": {
        "url": "https://github.com/idleberg/vscode-badgen/issues"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "markdown",
                "path": "./snippets/live.json"
            },
            {
                "language": "markdown",
                "path": "./snippets/static.json"
            }
        ]
    },
    "devDependencies": {
        "gulp": "^4.0.0",
        "gulp-raster": "^0.2.0",
        "gulp-rename": "^1.2.2",
        "husky": "^0.14.3",
        "vscode": "^1.0.3",
        "vscode-linter": "^0.2.1"
    }
}
